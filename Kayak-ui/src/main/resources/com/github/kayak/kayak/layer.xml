<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="com-github-kayak-ui-descriptions-DescriptionsAction.instance">
                <attr name="component" methodvalue="com.github.kayak.ui.descriptions.DescriptionsTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="com.github.kayak.ui.descriptions.Bundle#CTL_DescriptionsAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
                <attr name="iconBase" stringvalue="org/freedesktop/tango/16x16/mimetypes/text-x-generic.png"/>
            </file>
            <file name="com-github-kayak-ui-startpage-StartPageAction.instance">
                <attr name="component" methodvalue="com.github.kayak.ui.startpage.StartPageTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="com.github.kayak.ui.startpage.Bundle#CTL_StartPageAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
    </folder>

    <folder name="Menu">
        <folder name="File">
            <attr name="position" intvalue="0"/>
        </folder>
        <file name="GoTo_hidden"/>
        <folder name="Tools">
            <file name="Separator1.instance_hidden"/>
            <file name="org-netbeans-modules-favorites-templates-TemplatesAction.shadow_hidden"/>
        </folder>
        <folder name="View">
            <file name="org-netbeans-core-actions-HTMLViewAction.shadow_hidden"/>
        </folder>
        <folder name="Window">
            <file name="DescriptionsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-github-kayak-ui-descriptions-DescriptionsAction.instance"/>
            </file>
            <file name="StartPageAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-github-kayak-ui-startpage-StartPageAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="BusStatisticsTopComponent.settings" url="BusStatisticsTopComponentSettings.xml"/>
            <file name="DescriptionsTopComponent.settings" url="DescriptionsTopComponentSettings.xml"/>
            <file name="StartPageTopComponent.settings" url="StartPageTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <file name="management.wsmode" url="management.wsmode"/>
           <!--<file name="statistics.wsmode" url="statistics.wsmode"/>-->
            <folder name="editor">
                <file name="StartPageTopComponent.wstcref" url="StartPageTopComponentWstcref.xml"/>
            </folder>

            <folder name="management">
                <file name="DescriptionsTopComponent.wstcref" url="DescriptionsTopComponentWstcref.xml"/>
            </folder>
            <!--<folder name="statistics">
                <file name="BusStatisticsTopComponent.wstcref" url="BusStatisticsTopComponentWstcref.xml"/>
            </folder>-->
            <folder name="properties">
                <file name="properties.wstcref" url="propertiesWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
